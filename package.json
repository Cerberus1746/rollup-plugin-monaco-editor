  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "rollup",
    "plugin",
    "monaco-editor"
  ],
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chengcyber/rollup-plugin-monaco-editor"
  },
  "scripts": {
    "start": "tsdx watch",
    "start:example": "http-server -c-1 example",
    "build": "tsdx build",
    "build:example": "(cd example && rollup -c rollup.config.js)",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {}
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "rollup-plugin-monaco-editor",
