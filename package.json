    {
      "path": "dist/rollup-plugin-monaco-editor.esm.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "monaco-editor": "0.21.2",
    "rollup": "^2.33.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@size-limit/preset-small-lib": "^4.7.0",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "monaco-editor": "0.21.2",
    "rollup": "^2.33.1",
    "rollup-plugin-postcss": "^3.1.8",
    "size-limit": "^4.7.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {}
